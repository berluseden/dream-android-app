    {"name": "Deadlift Intensity", "exercises": [
      {"name": "Conventional Deadlift", "sets": 5, "reps": "3-2", "rir": 1, "rest_seconds": 240},
      {"name": "Romanian Deadlift", "sets": 3, "reps": "4-6", "rir": 1, "rest_seconds": 180},
      {"name": "Back Extensions", "sets": 3, "reps": "10-12", "rir": 1, "rest_seconds": 90}
    ]},
    {"name": "Upper/Accessories", "exercises": [
      {"name": "Incline Dumbbell Press", "sets": 3, "reps": "6-8", "rir": 1, "rest_seconds": 150},
      {"name": "Seated Cable Row", "sets": 3, "reps": "8-10", "rir": 1, "rest_seconds": 120},
      {"name": "Face Pulls", "sets": 3, "reps": "15-20", "rir": 1, "rest_seconds": 75}
    ]}
  ]
}