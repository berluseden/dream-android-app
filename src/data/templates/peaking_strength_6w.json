{
  "name": "Peaking Strength 6× Weeks",
  "description": "Ciclo de pico de fuerza (6 semanas) con reducción de volumen y aumento de intensidad. Ideal para test de 1RM o mock meet.",
  "level": "advanced",
  "frequency": 4,
  "length_weeks": 6,
  "focus": "strength",
  "required_equipment": ["barbell", "rack"],
  "muscle_focus": ["chest", "back", "quads", "hamstrings"],
  "days": [
    {"name": "Bench Intensity", "exercises": [
      {"name": "Barbell Bench Press", "sets": 5, "reps": "3-2", "rir": 1, "rest_seconds": 240},
      {"name": "Close-Grip Bench", "sets": 3, "reps": "4-6", "rir": 1, "rest_seconds": 180},
      {"name": "Rear Delt Fly", "sets": 3, "reps": "15-20", "rir": 1, "rest_seconds": 75}
    ]},
    {"name": "Squat Intensity", "exercises": [
      {"name": "Back Squat", "sets": 5, "reps": "3-2", "rir": 1, "rest_seconds": 240},
      {"name": "Paused Squat", "sets": 3, "reps": "3-4", "rir": 1, "rest_seconds": 210},
      {"name": "Leg Curl", "sets": 3, "reps": "10-12", "rir": 1, "rest_seconds": 90}
